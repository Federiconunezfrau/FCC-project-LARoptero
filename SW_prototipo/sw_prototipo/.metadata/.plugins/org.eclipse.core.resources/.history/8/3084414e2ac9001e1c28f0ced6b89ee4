#include "taskWatchdog.h"

void taskWatchdog_constructor(taskWatchdog_t *me, uint32_t delayTicks, uint32_t periodTicks, uint32_t wcetMicroSeconds, uint32_t bcetMicroSeconds, gpio *led)
{
	timeTriggeredTask_constructor(&me->super, (taskHandler_t)&taskWatchdog_update, (taskHandler_t)&taskWatchdog_start, delayTicks, periodTicks, wcetMicroSeconds, bcetMicroSeconds);
	me->mLED_ = led;
}

void taskWatchdog_destructor  ( taskWatchdog_t * );
void taskWatchdog_start       ( taskWatchdog_t * );
void taskWatchdog_update      ( taskWatchdog_t * );



