#ifndef INC_UTILS_H_
#define INC_UTILS_H_

#include <stdint.h>

uint8_t *copy_byte_array(uint8_t *dest, const uint8_t *src, uint32_t n)
{
	uint32_t i;
	for(i = 0; i < n ; i++)
	{
		dest[i] = str[i];
	}
}


#endif /* INC_UTILS_H_ */
