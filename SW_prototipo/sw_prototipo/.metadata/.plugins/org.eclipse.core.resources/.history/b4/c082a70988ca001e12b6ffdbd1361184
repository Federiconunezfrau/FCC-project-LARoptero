#ifndef INC_CAN_CNI_H_
#define INC_CAN_CNI_H_

#include "stm32f7xx_hal.h"

typedef struct
{
	uint8_t  payload[MAX_LEN_PAYLOAD_CAN];
	uint32_t lenPayload;
	uint32_t id;
	uint32_t nodeID;
} CANmsg_t;

typedef struct
{
	CAN_HandleTypeDef *m_canHandler;


} CANcni_t;


#endif /* INC_CAN_CNI_H_ */
