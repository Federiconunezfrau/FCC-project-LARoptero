#ifndef INC_TIMETRIGGEREDTASK_H_
#define INC_TIMETRIGGEREDTASK_H_

#include <stdint.h>

typedef struct
{
	uint32_t mDelayTicks_;
	uint32_t mPeriodTicks_;
	uint32_t mWcetMicroSeconds_;
	uint32_t mBcetMicroSeconds_;
} timeTriggeredTask_t;



#endif /* INC_TIMETRIGGEREDTASK_H_ */
